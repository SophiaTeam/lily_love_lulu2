<p id="notice"><%= notice %></p>

<h1>Inbocs</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inbocs.each do |inboc| %>
      <tr>
        <td><%= inboc.title %></td>
        <td><%= inboc.content %></td>
        <td><%= inboc.user.name %></td>
        <td><%= link_to 'Show', inboc %></td>
        <td><%= link_to 'Edit', edit_inboc_path(inboc) %></td>
        <td><%= link_to 'Destroy', inboc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inboc', new_inboc_path %>
